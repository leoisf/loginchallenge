{
  "project-settings": {
    "code-owner": "Login Challenge",
    "bundle-path": "../core/src/main/java",
    "test-path": "../core/src/test/java",
    "apps-path": "../ui.apps/src/main/content/jcr_root/apps",
    "component-path": "loginchallenge",
    "model-interface-pkg": "com.loginchallenge.core.models",
    "model-impl-pkg": "com.loginchallenge.core.models.impl"
  },
  "name": "loginchallengehomefooter",
  "title": "Login Challenge Home Footer",
  "group": "Login Challenge - Content",
  "type": "components",
  "options": {
    "js": false,
    "jstxt": false,
    "css": false,
    "csstxt": false,
    "html": false,
    "html-content": false,
    "slingmodel": true,
    "testclass": false,
    "junit-major-version": 5,
    "content-exporter": true,
    "model-adaptables": [
      "request"
    ],
    "generic-javadoc": false,
    "properties": [
      {
        "field": "section_msg",
        "type": "textfield",
        "json-expose": true,
        "json-property": "text",
        "label": "Section message"
      },
      {
        "field": "aplication_refresh",
        "type": "textfield",
        "json-expose": true,
        "json-property": "text",
        "label": "Aplication refresh text"
      },
      {
        "field": "search_button_url",
        "type": "pathfield",
        "json-expose": true,
        "attributes": {
            "rootPath": "content/"
        },
        "label": "Search button URL"
      },
      {
        "field": "search_button",
        "type": "textfield",
        "json-expose": true,
        "json-property": "text",
        "label": "Search button text"
      },
      {
        "field": "logout_button_url",
        "type": "pathfield",
        "json-expose": true,
        "attributes": {
            "rootPath": "content/"
        },
        "label": "Logout button URL"
      },
      {
        "field": "logout_button",
        "type": "textfield",
        "json-expose": true,
        "json-property": "text",
        "label": "Logout button text"
      }      
    ]
  }
}