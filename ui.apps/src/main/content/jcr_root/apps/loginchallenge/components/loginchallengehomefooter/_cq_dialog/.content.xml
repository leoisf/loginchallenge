<jcr:root xmlns:jcr="http://www.jcp.org/jcr/1.0"
          xmlns:cq="http://www.day.com/jcr/cq/1.0"
          xmlns:granite="http://www.adobe.com/jcr/granite/1.0"
          xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
          xmlns:sling="http://sling.apache.org/jcr/sling/1.0"
          jcr:primaryType="nt:unstructured"
          jcr:title="Login Challenge Home Footer"
          sling:resourceType="cq/gui/components/authoring/dialog">
   <content jcr:primaryType="nt:unstructured"
            sling:resourceType="granite/ui/components/coral/foundation/container">
      <layout jcr:primaryType="nt:unstructured"
              margin="{Boolean}false"
              sling:resourceType="granite/ui/components/foundation/layouts/fixedcolumns"/>
      <items jcr:primaryType="nt:unstructured">
         <column jcr:primaryType="nt:unstructured"
                 sling:resourceType="granite/ui/components/coral/foundation/container">
            <items jcr:primaryType="nt:unstructured">
               <section_msg cq-msm-lockable="./section_msg"
                            fieldLabel="Section message"
                            jcr:primaryType="nt:unstructured"
                            name="./section_msg"
                            sling:resourceType="granite/ui/components/coral/foundation/form/textfield"/>
               <aplication_refresh cq-msm-lockable="./aplication_refresh"
                                   fieldLabel="Aplication refresh text"
                                   jcr:primaryType="nt:unstructured"
                                   name="./aplication_refresh"
                                   sling:resourceType="granite/ui/components/coral/foundation/form/textfield"/>
               <search_button_url cq-msm-lockable="./search_button_url"
                                  fieldLabel="Search button URL"
                                  jcr:primaryType="nt:unstructured"
                                  name="./search_button_url"
                                  rootPath="content/"
                                  sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"/>
               <search_button cq-msm-lockable="./search_button"
                              fieldLabel="Search button text"
                              jcr:primaryType="nt:unstructured"
                              name="./search_button"
                              sling:resourceType="granite/ui/components/coral/foundation/form/textfield"/>
               <logout_button_url cq-msm-lockable="./logout_button_url"
                                  fieldLabel="Logout button URL"
                                  jcr:primaryType="nt:unstructured"
                                  name="./logout_button_url"
                                  rootPath="content/"
                                  sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"/>
               <logout_button cq-msm-lockable="./logout_button"
                              fieldLabel="Logout button text"
                              jcr:primaryType="nt:unstructured"
                              name="./logout_button"
                              sling:resourceType="granite/ui/components/coral/foundation/form/textfield"/>
            </items>
         </column>
      </items>
   </content>
</jcr:root>
